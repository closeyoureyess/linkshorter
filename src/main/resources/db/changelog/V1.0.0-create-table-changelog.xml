<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="1" author="vsirik">

        <createSequence sequenceName="my_entity_sequence" startValue="1" incrementBy="5"/>

        <createTable tableName="source_link">
            <column name="id" type="BIGINT">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="base_source_link" type="VARCHAR">
                <constraints nullable="false"/>
            </column>
            <column name="unique_identifier" type="VARCHAR">
                <constraints nullable="false"/>
            </column>
            <column name="link_unique_identifier" type="VARCHAR">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
